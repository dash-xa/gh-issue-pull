{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install poetry && poetry install",
        "startCommand": "poetry run uvicorn main:app --host 0.0.0.0 --port 8000"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}
